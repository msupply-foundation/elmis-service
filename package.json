{
  "name": "@openmsupply/elmis-service",
  "version": "0.1.14",
  "description": "Module for integration mSupply with eSigl",
  "main": "build/index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "babel-node ./src/index.js",
    "test": "jest",
    "bailTest": "jest --bail",
    "build": "rimraf build/ && babel ./src --out-dir build/ --ignore ./node_modules,./.babelrc,./package.json,./npm-debug.log --copy-files"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "*.js": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmsupply/elmis-service.git"
  },
  "author": "Sussol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/openmsupply/elmis-service/issues"
  },
  "homepage": "https://github.com/openmsupply/elmis-service#readme",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jest": "^22.3.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^8.1.3",
    "prettier": "1.16.4"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "axios": "^0.18.0",
    "qs": "^6.6.0"
  }
}
